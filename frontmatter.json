{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "previewPath": null,
      "fields": [
        {
          "title": "layout",
          "name": "layout",
          "type": "string"
        },
        {
          "title": "title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "author",
          "name": "author",
          "type": "string"
        },
        {
          "title": "categories",
          "name": "categories",
          "type": "categories"
        },
        {
          "title": "tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "image",
          "name": "image",
          "type": "image"
        },
        {
          "title": "featured",
          "name": "featured",
          "type": "boolean"
        },
        {
          "title": "rating",
          "name": "rating",
          "type": "number"
        },
        {
          "title": "description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "date",
          "name": "date",
          "type": "datetime"
        },
        {
          "title": "preview",
          "name": "preview",
          "type": "string"
        },
        {
          "title": "keywords",
          "name": "keywords",
          "type": "string"
        },
        {
          "title": "toc",
          "name": "toc",
          "type": "boolean"
        },
        {
          "title": "published",
          "name": "published",
          "type": "boolean"
        },
        {
          "title": "draft",
          "name": "draft",
          "type": "datetime"
        },
        {
          "title": "beforetoc",
          "name": "beforetoc",
          "type": "string"
        },
        {
          "title": "lastmod",
          "name": "lastmod",
          "type": "datetime"
        },
        {
          "title": "schema",
          "name": "schema",
          "type": "fields",
          "fields": [
            {
              "title": "type",
              "name": "type",
              "type": "string"
            },
            {
              "title": "name",
              "name": "name",
              "type": "string"
            },
            {
              "title": "description",
              "name": "description",
              "type": "string"
            },
            {
              "title": "steps",
              "name": "steps",
              "type": "block",
              "fieldGroup": [
                "steps_group"
              ]
            }
          ]
        }
      ]
    }
  ],
  "frontMatter.framework.id": "jekyll",
  "frontMatter.content.publicFolder": "assets",
  "frontMatter.preview.host": "http://localhost:4000",
  "frontMatter.content.pageFolders": [
    {
      "title": "_posts",
      "path": "[[workspace]]/_posts"
    },
    {
      "title": "_posts",
      "path": "[[workspace]]/_posts"
    }
  ],
  "frontMatter.content.snippets": {},
  "frontMatter.media.defaultSorting": "LastModifiedAsc",
  "frontMatter.taxonomy.fieldGroups": [
    {
      "id": "steps_group",
      "fields": [
        {
          "title": "name",
          "name": "name",
          "type": "string"
        },
        {
          "title": "description",
          "name": "description",
          "type": "string"
        }
      ]
    }
  ]
}