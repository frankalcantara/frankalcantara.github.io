{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "previewPath": null,
      "fields": [
        {
          "title": "layout",
          "name": "layout",
          "type": "string"
        },
        {
          "title": "title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "author",
          "name": "author",
          "type": "string"
        },
        {
          "title": "categories",
          "name": "categories",
          "type": "categories"
        },
        {
          "title": "tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "image",
          "name": "image",
          "type": "image"
        },
        {
          "title": "featured",
          "name": "featured",
          "type": "datetime"
        },
        {
          "title": "rating",
          "name": "rating",
          "type": "number"
        },
        {
          "title": "description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "date",
          "name": "date",
          "type": "datetime"
        },
        {
          "title": "preview",
          "name": "preview",
          "type": "string"
        },
        {
          "title": "keywords",
          "name": "keywords",
          "type": "choice",
          "choices": [
            "lógica",
            "Programação",
            "programação lógica"
          ]
        },
        {
          "title": "draft",
          "name": "draft",
          "type": "datetime"
        }
      ]
    }
  ],
  "frontMatter.framework.id": "jekyll",
  "frontMatter.content.publicFolder": "assets",
  "frontMatter.preview.host": "http://localhost:4000",
  "frontMatter.content.pageFolders": [
    {
      "title": "_posts",
      "path": "[[workspace]]/_posts"
    },
    {
      "title": "_posts",
      "path": "[[workspace]]/_posts"
    }
  ],
  "frontMatter.taxonomy.tags": [
    "C++",
    "Eletromagnetismo",
    "Física",
    "Matemática",
    "algoritmos",
    "covid",
    "eng. software",
    "estrutura de dados",
    "funcional",
    "inteligência artificial",
    "interpretadores",
    "linguagem natural",
    "lógica",
    "modelagem",
    "opinião",
    "orientação a objetos",
    "paradigmas de programação",
    "parsers",
    "predicativa",
    "programação",
    "proposicional",
    "resolução de problemas",
    "uml"
  ],
  "frontMatter.taxonomy.categories": [
    "Eletromagnetismo",
    "Matemática",
    "artigo",
    "disciplina",
    "opinião"
  ]
}